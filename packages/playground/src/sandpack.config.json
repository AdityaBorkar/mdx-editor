{
  "defaultPreset": "react-virtuoso",
  "presets": [
    {
      "name": "react-virtuoso",
      "sandpackTemplate": "react-tsx",
      "sandpackTheme": "dracula",
      "snippetFileName": "/App.tsx",
      "dependencies": {
        "react": "latest",
        "react-dom": "latest",
        "react-virtuoso": "latest"
      },
      "additionalDependencySets": [
        {
          "name": "emotion",
          "dependencies": {
            "@emotion/styled": "latest",
            "@emotion/react": "latest"
          }
        }
      ],
      "additionalFileSets": [{ "name": "data", "files": { "./data.ts": "./data.ts" } }]
    }
  ]
}
